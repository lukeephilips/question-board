<h1>Hello</h1>
{{#each model.questions as |question|}}
  <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{question.content}}</span>
          <p>{{question.notes}}</p>
          <h5>{{question.author}}</h5>
        </div>
        <div class="card-action">
          {{#each question.answer}}
          {{/each}}
        </div>
      </div>
    </div>
  </div>
{{/each}}
